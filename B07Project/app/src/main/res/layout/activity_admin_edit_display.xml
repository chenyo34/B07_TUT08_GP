<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminEditDisplay">

    <EditText
        android:id="@+id/adminEditTargetCourseCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:ems="10"
        android:hint="adminEditCourseCode"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adminEditCourseCodeTextview" />

    <EditText
        android:id="@+id/coursePre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:ems="10"
        android:hint="adminEditCoursePrereq"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adminEditCoursePrereqTextview" />

    <Button
        android:id="@+id/adminCourseDelete"
        android:layout_width="137dp"
        android:layout_height="49dp"
        android:layout_marginTop="30dp"
        android:text="Delete"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/coursePre" />

    <EditText
        android:id="@+id/adminEditTargetCourseName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:ems="10"
        android:hint="adminEditCourseName"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adminEditCourseNameTextview" />

    <TextView
        android:id="@+id/adminEditCourseCodeTextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Course Code"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/adminEditCourseNameTextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Course Name"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adminEditTargetCourseCode" />

    <TextView
        android:id="@+id/adminEditCourseOfferingTextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Course Offering"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/adminEditCoursePrereqTextview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adminEditTargetCourseName"
        app:layout_constraintVertical_bias="0.009" />

    <TextView
        android:id="@+id/adminEditCoursePrereqTextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="232dp"
        android:text="Course Pre Requirement"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.227"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/admitCourseListDisplayPreviousButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/previous"
        tools:layout_editor_absoluteX="16dp"

        tools:layout_editor_absoluteY="16dp" />

    <CheckBox
        android:id="@+id/checkBoxWinter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="30dp"
        android:text="Winter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adminEditCourseOfferingTextview" />

    <CheckBox
        android:id="@+id/checkBoxSummer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:text="Summer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adminEditCourseOfferingTextview" />

    <CheckBox
        android:id="@+id/checkBoxFall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:text="Fall"
        app:layout_constraintEnd_toStartOf="@+id/checkBoxWinter"
        app:layout_constraintStart_toEndOf="@+id/checkBoxSummer"
        app:layout_constraintTop_toBottomOf="@+id/adminEditCourseOfferingTextview" />

    <Button
        android:id="@+id/ChangesSaver"
        android:layout_width="140dp"
        android:layout_height="48dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="30dp"
        android:text="Save"
        app:layout_constraintStart_toEndOf="@+id/adminCourseDelete"
        app:layout_constraintTop_toBottomOf="@+id/coursePre" />

</androidx.constraintlayout.widget.ConstraintLayout>